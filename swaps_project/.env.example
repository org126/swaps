# Environment Configuration Example
# Copy this file to .env and update with your actual values
# IMPORTANT: Never commit .env to version control!

# Main Database Configuration (database.sql)
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASS=
DB_NAME=swaps

# Sagana Part Database Configuration (sagana_part.sql)
# Used by: Main_Report.php, technician.php
SAGANA_DB_HOST=127.0.0.1
SAGANA_DB_PORT=3306
SAGANA_DB_USER=root
SAGANA_DB_PASS=
SAGANA_DB_NAME=secure_web

# Security Configuration
REQUIRE_HTTPS=false

# Application Settings
SESSION_TIMEOUT=3600
